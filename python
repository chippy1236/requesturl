url  =  'https://s57.in/wp-content/uploads/2020/05/s57-png.png'
r  =  yêu cầu . get ( url , allow_redirects = True )

# Lưu nội dung với tên.
mở ( 'logo.png' , 'wb' ). write ( r . nội dung )

#running Tệp
hệ điều hành . startfile ( "logo.png" )
